{
  "name": "@MikeyAlmighty/raastaurant-ui",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MikeyAlmighty/raastaurant-ui.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
    "preview": "vite preview",
    "release": "standard-version",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test-watch": "vitest",
    "test:ui": "vitest --ui",
    "build-storybook": "storybook build",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "dependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.4",
    "@semantic-release/npm": "^11.0.1",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "framer-motion": "^10.16.16",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.1",
    "semantic-release": "^22.0.8"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "author": "Michael Leibbrandt",
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@storybook/addon-essentials": "^7.6.2",
    "@storybook/addon-interactions": "^7.6.2",
    "@storybook/addon-links": "^7.6.2",
    "@storybook/blocks": "^7.6.2",
    "@storybook/react": "^7.6.2",
    "@storybook/react-vite": "^7.6.2",
    "@storybook/test": "^7.6.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "semantic-release": "^22.0.10",
    "storybook": "^7.6.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.4",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^0.34.6"
  },
  "files": [
    "/dist"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E  HUSKY_GIT_PARAMS"
    }
  }
}
