{
  "name": "raastaurant-ui",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test-watch": "vitest",
    "test:ui": "vitest --ui",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format",
      "npm run lint",
      "npm run test"
    ]
  },
  "dependencies": {
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "author": "Michael Leibbrandt",
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.2",
    "@storybook/addon-interactions": "^7.6.2",
    "@storybook/addon-links": "^7.6.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.6.2",
    "@storybook/react": "^7.6.2",
    "@storybook/react-vite": "^7.6.2",
    "@storybook/test": "^7.6.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "storybook": "^7.6.2",
    "styled-components": "^6.1.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.4",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^0.34.6"
  },
  "files": [
    "/dist"
  ]
}
